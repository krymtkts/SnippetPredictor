      <maml:para>Remove specified snippets from the snippet configuration. The snippet configuration is located at `~/.snippet-predictor.json`.</maml:para>
    </maml:description>
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Remove-Snippet</maml:name>
        <command:parameter required="true" variableLength="false" globbing="false" pipelineInput="false" position="0" aliases="none">
